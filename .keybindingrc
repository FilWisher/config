## keybindings file for sxhkd ##

# start applications
super + o; {g,r,t}
  {chromium, roxterm, roxterm -e tmux}

control + super + {h,l}
  tmux {previous-window,next-window}
  
control + alt + {h,l,j,k}
  tmux resize-pane -{L,R,D,U}
  
super + {y,p}
  xsel {-i,-o}
  
super + {F1}
  xdotool getwindowfocus key F11
  
super + {d,u}
  {dv,gt}
